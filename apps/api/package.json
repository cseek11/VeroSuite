{
    "name":  "@verofield/api",
    "version":  "1.0.0",
    "private":  true,
    "description":  "VeroField Backend API - NestJS multi-tenant with RLS",
    "license":  "MIT",
    "scripts":  {
                    "start":  "node dist/apps/api/src/main.js",
                    "start:dev":  "nest build --watch \u0026 timeout /t 3 /nobreak \u003enul \u0026 node dist/apps/api/src/main.js",
                    "start:dev:fast":  "nest start --watch --preserveWatchOutput --watchAssets false",
                    "start:prod":  "node dist/apps/api/src/main.js",
                    "start:test":  "NODE_ENV=test nest start",
                    "build":  "nest build",
                    "typecheck":  "tsc -p ../../tsconfig.backend.json --noEmit",
                    "test":  "jest",
                    "test:watch":  "jest --watch",
                    "test:coverage":  "jest --coverage",
                    "test:unit":  "jest --config ./test/enterprise-testing.config.js --testPathPattern=\"(__tests__|test/unit)\"",
                    "test:integration":  "jest --config ./test/enterprise-testing.config.js --testPathPattern=integration",
                    "test:security":  "jest --config ./test/enterprise-testing.config.js --testPathPattern=security",
                    "test:performance":  "jest --config ./test/enterprise-testing.config.js --coverage=false --testPathPattern=performance",
                    "test:e2e":  "jest --config ./test/jest-e2e.json",
                    "test:component":  "jest --config ./test/enterprise-testing.config.js --testPathPattern=component",
                    "test:workflow":  "jest --config ./test/enterprise-testing.config.js --testPathPattern=workflow",
                    "test:all":  "npm run test:unit \u0026\u0026 npm run test:integration \u0026\u0026 npm run test:security \u0026\u0026 npm run test:performance \u0026\u0026 npm run test:e2e",
                    "test:setup":  "npm run db:generate \u0026\u0026 npm run db:push",
                    "test:quality:report":  "jest --config ./test/enterprise-testing.config.js --coverage --coverageReporters=html,text,lcov",
                    "test:performance:thresholds":  "k6 run test/performance/load-testing.js",
                    "test:security:score":  "npm audit --audit-level=high",
                    "load:test":  "k6 run test/performance/load-testing.js",
                    "stress:test":  "k6 run test/performance/stress-testing.js",
                    "dashboard:generate":  "node scripts/generate-dashboard.js",
                    "dashboard:open":  "start coverage/testing-dashboard.html",
                    "db:generate":  "prisma generate --schema=../../libs/common/prisma/schema.prisma",
                    "db:push":  "prisma db push --schema=../../libs/common/prisma/schema.prisma",
                    "db:migrate":  "prisma migrate dev --schema=../../libs/common/prisma/schema.prisma",
                    "db:seed":  "ts-node ../../libs/common/prisma/seed.ts"
                },
    "dependencies":  {
                         "@nestjs/common":  "^10.0.0",
                         "@nestjs/config":  "^3.0.0",
                         "@nestjs/core":  "^10.0.0",
                         "@nestjs/event-emitter":  "^2.0.0",
                         "@nestjs/jwt":  "^10.1.0",
                         "@nestjs/passport":  "^10.0.0",
                         "@nestjs/platform-express":  "^10.0.0",
                         "@nestjs/platform-socket.io":  "^10.4.20",
                         "@nestjs/schedule":  "^3.0.0",
                         "@nestjs/swagger":  "^7.1.0",
                         "@nestjs/websockets":  "^10.4.20",
                         "@prisma/client":  "^5.0.0",
                         "@stripe/stripe-js":  "^7.9.0",
                         "@supabase/supabase-js":  "^2.38.0",
                         "@verofield/common":  "file:../../libs/common",
                         "@types/multer":  "^2.0.0",
                         "@types/redis":  "^4.0.10",
                         "@types/socket.io":  "^3.0.1",
                         "axios":  "^1.11.0",
                         "bcrypt":  "^5.1.0",
                         "class-transformer":  "^0.5.1",
                         "class-validator":  "^0.14.0",
                         "dotenv":  "^17.2.2",
                         "multer":  "^2.0.2",
                         "passport":  "^0.6.0",
                         "passport-jwt":  "^4.0.1",
                         "pg":  "^8.16.3",
                         "redis":  "^5.8.2",
                         "reflect-metadata":  "^0.1.14",
                         "rxjs":  "^7.8.2",
                         "socket.io":  "^4.8.1",
                         "stripe":  "^18.5.0",
                         "uuid":  "^9.0.0"
                     },
    "devDependencies":  {
                            "@nestjs/cli":  "^11.0.10",
                            "@nestjs/testing":  "^10.0.0",
                            "@types/bcrypt":  "^5.0.0",
                            "@types/jest":  "^29.5.0",
                            "@types/passport-jwt":  "^3.0.9",
                            "@types/supertest":  "^6.0.0",
                            "jest":  "^29.5.0",
                            "jest-html-reporters":  "^3.1.5",
                            "jest-junit":  "^16.0.0",
                            "jest-watch-typeahead":  "^2.2.2",
                            "k6":  "^0.0.0",
                            "prisma":  "^5.0.0",
                            "supertest":  "^6.3.0",
                            "ts-jest":  "^29.1.0",
                            "ts-node":  "^10.9.1",
                            "typescript":  "^5.1.3",
                            "vitest":  "^3.2.4"
                        },
    "engines":  {
                    "node":  "\u003e=18.0.0"
                }
}