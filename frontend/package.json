    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-input-mask": "^2.0.4",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.27.0",
    "recharts": "^3.1.2",
